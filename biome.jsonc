{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"organizeImports": { "enabled": true },
  "files": {
    "ignore": [
      "package*.json",
      "lib",
      "dist"
    ]
  },
	"formatter": { "enabled": true },
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noExplicitAny": "warn"
			},
			"correctness": {
				"noUnusedImports": "error"
			}
		}
	}
}
